    <td>
      <span tal:repeat="student students"><a tal:attributes="href request.route_path('user_edit', user_id=student.id)">${student}
        <span tal:condition="student.is_deletable()"> (l√∂schbar)</span></a><br /></span>
    </td>
  </tr>
</table>

  </metal:main>
</metal:main>
