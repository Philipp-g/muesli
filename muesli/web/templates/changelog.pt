<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN" "http://www.w3.org/TR/html4/strict.dtd">
<metal:main metal:use-macro="templates('Fragments/main.pt').macros['main']">
  <metal:main metal:fill-slot="content">
<h2>Neuigkeiten bei MÃœSLI</h2>

<table>
  <tr tal:repeat="entry entries">
    <td class="changelog_date">
      ${entry['date']}
    </td>
    <td class="changelog_description">
      <div tal:repeat="subpart entry['content']">
        <ul tal:condition="type(subpart) is dict">
          <li tal:condition="subpart.get('listitems','')" tal:repeat="item subpart['listitems']">
            ${item}
          </li>
        </ul>
        <div tal:condition="type(subpart) is not dict">
          ${subpart} <br>
        </div>
      </div>
    </td>
  </tr>
</table>

  </metal:main>
</metal:main>
